{
  "name": "12.Individual-project",
  "version": "1.0.0",
  "description": "",
  "main": "index.html",
  "scripts": {
    "test": "npm-run-all test:*",
    "test:html": "globstar nu-html-checker src/*.html",
    "start:project": "npm install && npm-run-all init:*",
    "init:dirs": "mkdirp sass css vendor images js",
    "init:files": "touch README.md index.html sass/style.scss js/script.js",
    "init:gitignore": "curl https://raw.githubusercontent.com/github/gitignore/master/Node.gitignore -o .gitignore",
    "build": "npm-run-all build:* test",
    "build:clean": "rimraf src/css/style.css",
    "build:sass": "node-sass --output-style compact -o src/css src/sass",
    "build:autoprefixer": "autoprefixer-cli src/css/style.css",
    "build-dev:sass": "node-sass --output-style expanded --source-map true -o src/css src/sass",
    "watch": "npm-run-all build -p watch:*",
    "watch:sassprefixer": "node-sass --output-style expanded --source-map true -o src/css src/sass --watch && onchange -d 100 src/css/style.css -- autoprefixer-cli -o src/css/style.prefixed.css src/css/style.css",
    "watch:browsersync": "node_modules/.bin/browser-sync start --server --files src/css/*.css src/*.html"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/MichalAW/12.Individual-project.git"
  },
  "keywords": [],
  "author": "MichalAW michal.wanta@gmail.com",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/MichalAW/12.Individual-project/issues"
  },
  "homepage": "https://github.com/MichalAW/12.Individual-project#readme",
  "devDependencies": {
    "autoprefixer-cli": "^1.0.0",
    "browser-sync": "^2.26.3",
    "globstar": "^1.0.0",
    "node-sass": "^4.11.0",
    "onchange": "^5.2.0",
    "parcel-bundler": "^1.12.3",
    "posthtml-modules": "^0.4.2"
  },
  "dependencies": {
    "npm-run-all": "^4.1.5",
    "nu-html-checker": "^0.1.0",
    "sass": "^1.16.0",
    "save-dev": "^2.0.0"
  }
}
